pipeline{
  agent {
      docker{
          image 'node:14-alpine'
      }
  }
  stages{
    stage('install'){
        steps{
            sh 'npm install -f'
        }
    }  
    stage('Build'){
      steps{
          sh 'npm run build'
      }  
    }
    stage('Test'){
      steps{
          sh 'npm run coverage'
      }  
    }
}

}
