pipeline{
  agent any
  stages{
    stage('Installation'){
      steps{
            sh 'sudo apt-get install nodejs'
          }
      }
    stage('Build'){
      steps{
          echo "Second step"
      }  
    }
    stage('ESlint'){
      steps{
          echo "Third AKA Eslint step"
      }  
    }
}

}
