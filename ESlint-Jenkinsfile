pipeline{
  agent any
  tools {nodejs}
  stages{
    stage('Installation'){
      steps{
        script{
          sh 'npm install'
        }
      }
    }
    stage('Build'){
      steps{
        script{
          sh 'npm run build'
        }
      }  
    }
    stage('ESlint'){
      steps{
        script{
          sh 'npm run lint'
        }
      }  
    }
}
}
