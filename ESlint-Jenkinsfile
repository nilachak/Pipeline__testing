pipeline{
  agent any
  stages{
    stage('Installation'){
      steps{
            nodejs(nodeJSInstallationName: 'Node 6.x', configId: '<config-file-provider-id>') {
                    sh 'npm config ls'
                }
          }
      }
    stage('Build'){
      steps{
          echo "Second step"
      }  
    }
    stage('ESlint'){
      steps{
          echo "Third AKA Eslint step"
      }  
    }
}

}
